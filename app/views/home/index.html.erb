<h3>Welcome <%= @username %></h3>
<h4>Points Earned : <%= @points %>

  <p></p>
  <% if @checkins.empty? %>
    <%= "<p>You have not attended any events yet</p>" %>
  <% else %>
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Event</th>
          <th>Type</th>
        </tr>
      </thead>
    
      <tbody>
        <% @checkins.each do |checkin| %>
          <tr>
            <td><%= Event.find(checkin.event_id).name %></td>
            <td><%= Event.find(checkin.event_id).type_id %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  <% end %>

